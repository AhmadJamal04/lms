{
  "name": "node-boiler-plate-app",
  "version": "0.0.1",
  "description": "Boiler Plate Application of Node",
  "scripts": {
    "start": "nodemon server.js",
    "server": "nodemon server.js | ./node_modules/.bin/bunyan",
    "start-server": "./node_modules/pm2/bin/pm2 start server.js",
    "stop-server": "./node_modules/pm2/bin/pm2 stop server.js",
    "logs": "./node_modules/pm2/bin/pm2 logs",
    "test": "npm run test",
    "lint": "npx eslint .",
    "prettier": "npx prettier -c .",
    "db:migrate": "npx sequelize db:migrate",
    "db:migrate:create": "npx sequelize migration:generate --name",
    "db:migrate:undo": "npx sequelize db:migrate:undo",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+repo_url"
  },
  "author": "",
  "license": "ISC",
  "homepage": "repo_url#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-storage": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-bunyan-logger": "^1.3.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.2",
    "nodemailer": "^6.9.8",
    "pm2": "^5.3.0",
    "sequelize": "^6.31.1",
    "sharp": "^0.32.6",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
